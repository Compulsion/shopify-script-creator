{
  "name": "shopify-script-creator",
  "private": true,
  "version": "0.9.0",
  "description": "Shopify Script Creator",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "prebuild": "rimraf ./docs",
    "build": "webpack",
    "predeploy": "yarn run build",
    "deploy": "git add ./docs && git commit -m 'Update build' && git push && git subtree split --prefix docs -b gh-pages && git push deploy gh-pages:master && git branch -D gh-pages"
  },
  "dependencies": {
    "@shopify/polaris": "^1.9.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.8",
    "html-webpack-plugin": "^2.30.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "license": "MIT"
}
